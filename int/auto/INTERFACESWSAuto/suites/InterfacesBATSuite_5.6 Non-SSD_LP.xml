		<parameter name="testGroup" value="INTERFACES BAT RUN ON 5.6.x LP CENTRAL"/>
		<groups>
			<run>
				<include name="bat" />
			</run>
		</groups>
		<packages>
			<package name="com.oracle.hgbu.opera.qaauto.ws.testcases.ows.*" />
		</packages>
	</test> 
	<test name="ADS BAT Suite 5.6.x LP ASP" preserve-order="true">
		<!-- Pass parameters here -->
		<parameter name="environment" value="LAUNCHPAD_5.6.x_ASP" />
		<parameter name="runOnEntry" value="ES_02" />
		<parameter name="wsLayer" value="OEDS" />	
		<!--  Possible Values ASP, NONASP, CENTRAL -->	
		<parameter name="schema" value="ASP"/>
		<parameter name="testGroup" value="INTERFACES BAT RUN ON 5.6.x LP ASP"/>
		<groups>
			<run>
				<include name="bat" />
			</run>
		</groups>
		<packages>
			<package name="com.oracle.hgbu.opera.qaauto.ws.testcases.ads.*" />
		</packages>
	</test>
	<test name="OWS BAT Suite 5.6.x LP ASP" preserve-order="true">
		<!-- Pass parameters here -->
		<parameter name="environment" value="LAUNCHPAD_5.6.x_ASP" />
		<parameter name="runOnEntry" value="ES_01" />
		<parameter name="wsLayer" value="OEDS" />	
		<!--  Possible Values ASP, NONASP, CENTRAL -->	
		<parameter name="schema" value="ASP"/>
		<parameter name="testGroup" value="INTERFACES BAT RUN ON 5.6.x LP ASP"/>
		<groups>
			<run>
				<include name="bat" />
			</run>
		</groups>
		<packages>
			<package name="com.oracle.hgbu.opera.qaauto.ws.testcases.ows.*" />
		</packages>
	</test>
</suite>

